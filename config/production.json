{
  "environment": "production",
  "debug": false,
  "logging": {
    "level": "INFO",
    "file": "logs/hikyuu_ui.log",
    "max_size": "100MB",
    "backup_count": 5
  },
  "database": {
    "type": "duckdb",
    "path": "data/hikyuu_production.duckdb",
    "pool_size": 10,
    "timeout": 30
  },
  "cache": {
    "type": "memory",
    "max_size_mb": 512,
    "ttl_seconds": 3600
  },
  "performance": {
    "max_workers": 16,
    "memory_limit_mb": 2048,
    "startup_timeout": 60
  },
  "security": {
    "enable_auth": false,
    "rate_limiting": true,
    "max_requests_per_minute": 1000
  }
}