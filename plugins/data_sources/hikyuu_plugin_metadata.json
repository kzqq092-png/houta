{
  "plugin_info": {
    "id": "hikyuu_data_source",
    "name": "HIkyuu数据源插件",
    "version": "1.0.0",
    "description": "基于HIkyuu框架的高性能量化数据源，支持股票、指数、基金等多资产类型数据获取",
    "author": "HIkyuu-UI Team",
    "homepage": "https://github.com/fasiondog/hikyuu",
    "repository": "https://github.com/fasiondog/hikyuu",
    "license": "MIT",
    "keywords": ["hikyuu", "quantitative", "data-source", "stock", "trading"],
    "category": "data_source",
    "type": "data_source"
  },
  "capabilities": {
    "supported_asset_types": [
      "stock",
      "index", 
      "fund"
    ],
    "supported_data_types": [
      "historical_kline",
      "real_time_quote",
      "fundamental"
    ],
    "supported_markets": [
      "SH",
      "SZ", 
      "BJ"
    ],
    "supported_frequencies": [
      "1m",
      "5m",
      "15m", 
      "30m",
      "60m",
      "D",
      "W",
      "M"
    ],
    "features": {
      "real_time_support": true,
      "historical_data": true,
      "fundamental_data": true,
      "technical_indicators": false,
      "news_data": false,
      "options_data": false,
      "futures_data": false,
      "crypto_data": false,
      "forex_data": false
    },
    "limits": {
      "max_history_years": 20,
      "max_symbols_per_request": 100,
      "rate_limit_per_minute": 1000,
      "concurrent_requests": 10
    }
  },
  "requirements": {
    "python_version": ">=3.8",
    "dependencies": [
      "hikyuu>=2.6.0",
      "pandas>=1.3.0",
      "numpy>=1.20.0"
    ],
    "system_requirements": {
      "memory_mb": 512,
      "disk_space_mb": 1024,
      "cpu_cores": 1
    }
  },
  "configuration": {
    "schema": {
      "type": "object",
      "properties": {
        "data_dir": {
          "type": "string",
          "default": "data",
          "description": "HIkyuu数据目录路径"
        },
        "preload_day_count": {
          "type": "integer",
          "default": 100000,
          "minimum": 1000,
          "maximum": 500000,
          "description": "预加载K线数据天数"
        },
        "enable_spot_agent": {
          "type": "boolean",
          "default": true,
          "description": "是否启用实时行情代理"
        },
        "log_level": {
          "type": "string",
          "enum": ["DEBUG", "INFO", "WARNING", "ERROR"],
          "default": "INFO",
          "description": "日志级别"
        },
        "timeout_seconds": {
          "type": "integer",
          "default": 30,
          "minimum": 5,
          "maximum": 300,
          "description": "请求超时时间（秒）"
        },
        "cache_enabled": {
          "type": "boolean",
          "default": true,
          "description": "是否启用缓存"
        },
        "cache_ttl_minutes": {
          "type": "integer",
          "default": 5,
          "minimum": 1,
          "maximum": 60,
          "description": "缓存生存时间（分钟）"
        }
      },
      "required": ["data_dir"]
    },
    "ui_schema": {
      "data_dir": {
        "ui:widget": "file",
        "ui:options": {
          "directory": true
        }
      },
      "preload_day_count": {
        "ui:widget": "range"
      },
      "log_level": {
        "ui:widget": "select"
      },
      "timeout_seconds": {
        "ui:widget": "range"
      },
      "cache_ttl_minutes": {
        "ui:widget": "range"
      }
    }
  },
  "installation": {
    "install_command": "pip install hikyuu",
    "verify_command": "python -c \"import hikyuu; print('HIkyuu installed successfully')\"",
    "setup_instructions": [
      "1. 安装HIkyuu: pip install hikyuu",
      "2. 配置数据目录路径",
      "3. 首次运行会自动下载和初始化数据",
      "4. 确保有足够的磁盘空间（建议至少10GB）"
    ]
  },
  "documentation": {
    "readme_url": "https://hikyuu.readthedocs.io/",
    "api_docs_url": "https://hikyuu.readthedocs.io/en/latest/api.html",
    "examples": [
      {
        "title": "获取股票K线数据",
        "description": "演示如何获取股票的历史K线数据",
        "code": "plugin.get_kdata('sh000001', freq='D', count=100)"
      },
      {
        "title": "获取实时行情",
        "description": "演示如何获取股票的实时行情数据", 
        "code": "plugin.get_real_time_quotes(['sh000001', 'sz399001'])"
      },
      {
        "title": "获取股票列表",
        "description": "演示如何获取指定市场的股票列表",
        "code": "plugin.get_asset_list(AssetType.STOCK, market='SH')"
      }
    ]
  },
  "changelog": [
    {
      "version": "1.0.0",
      "date": "2024-08-16",
      "changes": [
        "初始版本发布",
        "支持股票、指数、基金数据获取",
        "支持多种K线频率",
        "支持实时行情数据",
        "支持基本面数据获取",
        "完整的连接管理和健康检查"
      ]
    }
  ],
  "support": {
    "issues_url": "https://github.com/fasiondog/hikyuu/issues",
    "documentation_url": "https://hikyuu.readthedocs.io/",
    "community_url": "https://github.com/fasiondog/hikyuu/discussions",
    "email": "support@hikyuu.org"
  }
} 